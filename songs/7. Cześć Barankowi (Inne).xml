  <lyrics>
    <verse name="v1">
      <lines><tag name="su">A</tag>         <tag name="su">H7</tag>       <tag name="su">E</tag>      <tag name="su">E7</tag><br/>Cześć Barankowi!<br/><tag name="su">A</tag>         <tag name="su"> H7</tag>                <tag name="su">E</tag>                      <tag name="su">cis</tag><br/>Cześć Temu, który umarł za nasz grzech.<br/>          <tag name="su">fis</tag>                            <tag name="su">H7</tag><br/>Lecz po trzech dniach On z martwych powstał,<br/><tag name="su">E</tag>                       <tag name="su">cis</tag><br/>Wiecznie rządzi już!<br/>    <tag name="su">fis</tag> <tag name="su">H7</tag><br/>Alleluja!</lines>
    </verse>
    <verse name="v2">
      <lines><tag name="su">A</tag>         <tag name="su">H7</tag>     <tag name="su">E</tag>       <tag name="su">E7</tag><br/>Cześć Barankowi!	<br/><tag name="su">A</tag>    <tag name="su">H7</tag>               <tag name="su">E</tag>              <tag name="su">cis</tag><br/>Błogosławione imię Jego jest!<br/><tag name="su">fis</tag>                 <tag name="su">H7</tag>                        <tag name="su">E</tag>                       <tag name="su">cis</tag><br/>Każdy musi hołd Mu złożyć, każdy wyznać, że,<br/>      <tag name="su">fis</tag>     <tag name="su">H7</tag><br/>Jezus Królem jest!</lines>
    </verse>
    <verse name="v3">
      <lines>      <tag name="su">E</tag> <tag name="su">A</tag>          <tag name="su">E</tag> <tag name="su">H7</tag>        <tag name="su">E</tag> <tag name="su">A</tag>          <tag name="su">E</tag> <tag name="su">H7</tag>        <tag name="su">A</tag> <tag name="su">E</tag><br/>Oto Pan, Oto Pan, Oto Pan, Oto Pan, Oto Pan, <br/>            <tag name="su">E</tag> <tag name="su">A</tag>               <tag name="su">E</tag> <tag name="su">H7</tag>              <tag name="su">E</tag> <tag name="su">A</tag>                <tag name="su">E</tag> <tag name="su">H7</tag><br/>To mój Pan, To mój Pan, To mój Pan,  To mój Pan, <br/>            <tag name="su">A</tag> <tag name="su">E</tag><br/>To mój Pan </lines>
    </verse>
